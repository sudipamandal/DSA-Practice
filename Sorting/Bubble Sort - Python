# Bubble Sort
def bubble_sort(arr):
  n = len(arr)
  for i in range(n):
    for j in range(0, n-i-1):
      if arr[j] > arr[j+1]:
        arr[j], arr[j+1] = arr[j+1], arr[j]

def main():
  arr = input("Enter a list: ")
  arr = list(map(int, arr.split()))
  bubble_sort(arr)
  print("Sorted array: ", arr)

if __name__ == "__main__":
  main()
